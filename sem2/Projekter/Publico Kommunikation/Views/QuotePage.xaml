<Page x:Class="Publico_Kommunikation_Project.Views.QuotePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Publico_Kommunikation_Project.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="QuotePage">

    <Grid>
         <StackPanel VerticalAlignment="Center"
            HorizontalAlignment="Center">
            
            <TextBlock Text="QuotePage"  
               Foreground="Black"
               FontSize="25"/>

            <Button Content="ProductsPage"
                    Height="25"
                    Command="{Binding NavigateToProductsViewCommand}"/>

        </StackPanel>
    </Grid>
</Page>
