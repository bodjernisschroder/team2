<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="DataAccess\Repositories\**" />
    <EmbeddedResource Remove="DataAccess\Repositories\**" />
    <None Remove="DataAccess\Repositories\**" />
    <Page Remove="DataAccess\Repositories\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="MVVM\Views\ProductsPage.xaml.cs" />
    <Compile Remove="MVVM\Views\QuotePage.xaml.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Appsettings" />
    <None Remove="Queries\tables_Senere.sql" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="MVVM\Views\ProductsPage.xaml" />
    <Page Remove="MVVM\Views\QuotePage.xaml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignInXamlToolkitAddOns" Version="0.0.6" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.2.2" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="9.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
	
	
	
</Project>
