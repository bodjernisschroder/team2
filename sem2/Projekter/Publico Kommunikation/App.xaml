<Application x:Class="Publico_Kommunikation_Project.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Publico_Kommunikation_Project"
             xmlns:view="clr-namespace:Publico_Kommunikation_Project.MVVM.Views"
             xmlns:viewModel="clr-namespace:Publico_Kommunikation_Project.MVVM.ViewModels">
<Application.Resources>
        <DataTemplate DataType="{x:Type viewModel:QuoteViewModel}">
            <view:QuotePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:ProductsViewModel}">
            <view:ProductsPage />
        </DataTemplate>
        
    </Application.Resources>
</Application>